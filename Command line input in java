import java.util.*;
/**
 * Cli
 */
public class Cli {

    public static void main(String[] args) {
        Scanner in=new Scanner(System.in);
        int i;
        System.out.println("The arguments you entered are as follows ");
        for(i=0;i<args.length;i++)
        {
            System.out.println(" "+args[i]);
        }   
    }
}
